            <Button Name="btninsert" Height="38" Width="103" TabIndex="0" Margin="23,4,984,602" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="btninsert_Click" >INSERT</Button>
        </Canvas>
        <Canvas Name="panel1" Height="238" Width="1086" Margin="2,-7,22,413" HorizontalAlignment="Right" VerticalAlignment="Bottom" >

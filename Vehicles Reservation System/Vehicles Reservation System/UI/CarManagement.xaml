            <DatePicker Name="datetimemfd" Height="29" Width="187" TabIndex="16" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="574" Canvas.Top="15" />
            <ComboBox Name="cmbcategory" Height="21" Width="186" TabIndex="15" Margin="132,126,792,497" HorizontalAlignment="Right" VerticalAlignment="Bottom" >
                <ComboBoxItem Content="Sedan"/>
                <ComboBoxItem Content="Jeep"/>
                <ComboBoxItem Content="Hybrid"/>
            </ComboBox>
            <TextBox Name="txtdeilyprice" Height="20" Width="187" TabIndex="14" Margin="574,188,349,436" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
            <TextBox Name="txtregno" Height="20" Width="187" TabIndex="13" Margin="574,128,349,496" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
            <TextBox Name="txtinsurance" Height="20" Width="187" TabIndex="12" Margin="574,68,349,556" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
            <TextBox Name="txtcolor" Height="20" Width="187" TabIndex="11" Margin="132,188,791,436" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
            <TextBox Name="txtcarname" Height="20" Width="187" TabIndex="10" Margin="132,71,791,553" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
            <TextBox Name="txtcarid" Height="20" Width="187" TabIndex="9" Margin="132,15,791,609" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" IsEnabled="False" ></TextBox>
            <Label Name="label9" Height="25" Width="100" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="8" Margin="30,128,980,491" HorizontalAlignment="Right" VerticalAlignment="Bottom" >CATEGORY</Label>
            <Label Name="label8" Height="25" Width="66" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="7" Margin="449,192,595,427" HorizontalAlignment="Right" VerticalAlignment="Bottom" >PRICE</Label>
            <Label Name="label7" Height="25" Width="68" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="6" Margin="30,192,1012,427" HorizontalAlignment="Right" VerticalAlignment="Bottom" >COLOR</Label>
            <Label Name="label6" Height="25" Width="75" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="5" Margin="440,128,595,491" HorizontalAlignment="Right" VerticalAlignment="Bottom" >REG NO</Label>
            <Label Name="label4" Height="25" Width="131" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="3" Margin="440,72,539,547" HorizontalAlignment="Right" VerticalAlignment="Bottom" >INSURANCE NO</Label>
            <Label Name="label3" Height="25" Width="94" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="2" Margin="440,19,576,600" HorizontalAlignment="Right" VerticalAlignment="Bottom" >MFD DATE</Label>
            <Label Name="label2" Height="25" Width="67" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="1" Margin="30,19,1013,600" HorizontalAlignment="Right" VerticalAlignment="Bottom" >CAR ID</Label>
            <Label Name="label1" Height="25" Width="95" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="0" Margin="30,72,985,547" HorizontalAlignment="Right" VerticalAlignment="Bottom" >CAR NAME</Label>
        </Canvas>
    </Grid>
</Controls:MetroWindow>

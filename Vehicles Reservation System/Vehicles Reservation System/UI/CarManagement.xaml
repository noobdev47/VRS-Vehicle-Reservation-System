<Controls:MetroWindow x:Name="Car_Window" x:Class="Vehicles_Reservation_System.CarManagement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        Background="{DynamicResource WindowBackgroundBrush}"
        TitleCaps="False"
	Title="Car Management" Width="1104" Height="665" Loaded="car_Window_Loaded">
    <Grid Height="644" Width="1110">
        <DataGrid Name="dgv4" Height="287" Width="1086" Margin="2,332,22,25" HorizontalAlignment="Right" VerticalAlignment="Bottom" >
            <DataGrid.Columns></DataGrid.Columns>
        </DataGrid>
        <Canvas Name="panel2" Height="89" Width="1086" Margin="2,237,22,318" HorizontalAlignment="Right" VerticalAlignment="Bottom" >

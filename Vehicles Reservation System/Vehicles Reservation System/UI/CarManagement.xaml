            <Button Name="btndisplay" Height="38" Width="103" TabIndex="4" Margin="733,4,274,602" HorizontalAlignment="Right" VerticalAlignment="Bottom" >DISPLAY</Button>
            <Button Name="btndeleteandsearchside" Height="38" Width="265" TabIndex="2" Margin="397,4,448,602" HorizontalAlignment="Right" VerticalAlignment="Bottom" >DELETE &amp; SEARCH SIDE</Button>

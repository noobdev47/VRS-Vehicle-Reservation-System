<Controls:MetroWindow x:Class="Vehicles_Reservation_System.UI.AdminDataGridWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        Background="{DynamicResource WindowBackgroundBrush}"
        TitleCaps="False"
        Title="VRS-Admins" x:Name="AdminGridDataWindow" Height="279" Width="304" Loaded="AdminGridDataWindow_Loaded">
    <Grid>
        <DataGrid x:Name="adminDataGrid" Margin="39,48,38,0" VerticalAlignment="Top" Height="178"/>
        <DockPanel HorizontalAlignment="Left" Height="27" LastChildFill="False" VerticalAlignment="Top" Width="296">
            <Label x:Name="lblLoggedIn" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Width="145" DockPanel.Dock="Right" Height="27"/>
            <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" Width="75" DockPanel.Dock="Left" Margin="0" BorderThickness="0" Background="{x:Null}" Click="btnBack_Click"/>
        </DockPanel>

    </Grid>
</Controls:MetroWindow>
